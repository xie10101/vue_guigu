    "pinia": "^2.2.2",
    "process": "^0.11.10",
    "progress": "^2.0.3",
    "vue": "^3.5.6",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.11.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "eslint": "^9.11.0",
    "eslint-config-prettier": "^9.1.0",
